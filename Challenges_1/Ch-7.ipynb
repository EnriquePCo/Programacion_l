     "name": "stdout",
     "output_type": "stream",
     "text": [
      "*\n",
      "****\n",
      "*****\n",
      "***\n",
      "****\n",
      "*****\n",
      "*******\n"
     ]
    }
   ],
   "source": [
    "l = [1,4,5,3,4,5,7]\n",
    "for i in range(len(l)):\n",
    "    f = '*'*l[i]\n",
    "    print(f)\n"
   ]
